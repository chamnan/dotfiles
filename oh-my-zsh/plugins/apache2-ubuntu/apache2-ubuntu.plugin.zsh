alias ap2ta='tail -f /var/log/apache2/access.log'
alias ap2te='tail -f /var/log/apache2/error.log'
alias ap2start='sudo service apache2 start'
alias ap2stop='sudo service apache2 stop'
alias ap2restart='sudo service apache2 restart'
alias ap2conf='sudo vim /etc/apache2/apache2.conf'
alias ap2portsconf='sudo vim /etc/apache2/ports.conf'

